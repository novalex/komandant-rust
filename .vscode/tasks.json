{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"subcommand": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Build"
		},
		{
			"type": "shell",
			"command": "cargo watch -x run",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Watch"
		}
	]
}